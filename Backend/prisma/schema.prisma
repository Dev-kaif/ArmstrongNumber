
generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String             @id @default(uuid())
  email      String             @unique
  name       String
  armstrongs ArmstrongNumber[]
}


model ArmstrongNumber {
  id        String   @id @default(uuid())
  number    Int
  userId    String
  user      User     @relation(fields: [userId], references: [id])
}
